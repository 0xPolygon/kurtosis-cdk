{
    "realVerifier": "{{.zkevm_use_real_verifier}}",
    "forkID": {{.zkevm_rollup_fork_id}},
    "programVKey": "{{.program_vkey}}",
    "description": "kurtosis-devnet - {{.chain_name}}",
    "adminZkEVM": "{{.l2_admin_address}}",
    "consensusContract": "{{.zkevm_rollup_consensus}}",
    "consensusContractName": "{{.zkevm_rollup_consensus}}",
    "type":"EOA",
    "trustedSequencerURL": "{{ .op_el_rpc_url }}",
    "networkName": "{{ .sovereign_chain_name }}",
    "trustedSequencer": "{{ .l2_sequencer_address }}",
    "chainID": "{{ .zkevm_rollup_chain_id }}",
    "rollupAdminAddress": "{{ .l2_sovereignadmin_address }}",
    "gasTokenAddress": "{{ .gas_token_address }}",
    "deployerPvtKey": "{{ .l2_admin_private_key }}",
    "aggchainManagerPvtKey": "{{ .l2_admin_private_key }}",
    "maxFeePerGas": "",
    "maxPriorityFeePerGas": "",
    "multiplierGas": "",
    "timelockDelay": 0,
    "timelockSalt": "",
    "rollupManagerAddress": "{{ .zkevm_rollup_manager_address }}",
    "rollupTypeId": 1,
    "proxiedTokensManager" : "{{ .l2_sovereignadmin_address }}",
    "_comment": "TODO.. It sounds like isVanillaClient should be false.. But setting it to false cause other issues. E.g. TypeError: newRollupContract.generateInitializeTransaction is not a function",
    "isVanillaClient": {{ .is_vanilla_client }},
    "sovereignParams": {
        "bridgeManager": "{{ .l2_sovereignadmin_address }}",
        "sovereignWETHAddress": "{{ .gas_token_address }}",
        "sovereignWETHAddressIsNotMintable": false,
        "globalExitRootUpdater": "{{ .l2_aggoracle_address }}",
        "globalExitRootRemover": "{{ .l2_sovereignadmin_address }}",
        "emergencyBridgePauser": "{{ .l2_sovereignadmin_address }}",
        "emergencyBridgeUnpauser": "{{ .l2_sovereignadmin_address }}",
        "proxiedTokensManager" : "{{ .l2_sovereignadmin_address }}"
    },
    "aggchainParams": {
        "aggchainManager": "{{ .l2_sovereignadmin_address }}",
        "aggchainManagerPvtKey": "{{.l2_sovereignadmin_private_key}}",
        "initParams": {
            "l2BlockTime": "{{ .sp1_l2_block_time }}",
            "rollupConfigHash": "{{.sp1_rollup_config_hash}}",
            "startingOutputRoot": "{{.sp1_starting_output_root}}",
            "startingBlockNumber": {{ with .sp1_starting_block_number }}{{ . }}{{ else }}null{{ end }},
            "startingTimestamp": {{ with .sp1_starting_timestamp}}{{ . }}{{ else }}null{{ end }},
            "submissionInterval": "{{ .sp1_submission_interval }}",
            "optimisticModeManager": "{{ .l2_sovereignadmin_address }}",
            "aggregationVkey": "{{ .sp1_aggregation_vkey }}",
            "rangeVkeyCommitment": "{{ .sp1_range_vkey_commitment }}"
        },
        "useDefaultVkeys": false,
        "ownedAggchainVKey": "{{ .aggchain_vkey_hash }}",
        "aggchainVKeySelector": "{{ .aggchain_vkey_selector }}",
        "initOwnedAggchainVKey": "{{ .aggchain_vkey_hash }}",
        "initAggchainVKeySelector": "{{ .aggchain_vkey_selector }}",
        "vKeyManager": "{{ .l2_sovereignadmin_address }}",
        "useDefaultSigners": false,
        "signers": [["{{ .l2_sequencer_address }}"," "]],
        "threshold": {{ .agg_sender_multisig_threshold }}
    }
}
