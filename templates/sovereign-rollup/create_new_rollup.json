{
    "realVerifier": "{{.zkevm_use_real_verifier}}",
    "forkID": "{{.fork_id}}",
    "programVKey": "{{.pp_vkey_hash}}",
    "description": "kurtosis-devnet - {{.chain_name}}",
    "adminZkEVM": "{{.zkevm_l2_admin_address}}",
    "consensusContract": "{{.zkevm_rollup_consensus}}",
    "consensusContractName": "{{.zkevm_rollup_consensus}}",
    "type":"EOA",
    "trustedSequencerURL": "{{ .op_el_rpc_url }}",
    "networkName": "{{ .sovereign_chain_name }}",
    "trustedSequencer": "{{ .zkevm_l2_sovereignadmin_address }}",
    "chainID": "{{ .zkevm_rollup_chain_id }}",
    "rollupAdminAddress": "{{ .zkevm_l2_sovereignadmin_address }}",
    "gasTokenAddress": "{{ .gas_token_address }}",
    "deployerPvtKey": "{{ .zkevm_l2_admin_private_key }}",
    "aggchainManagerPvtKey": "{{ .zkevm_l2_sovereignadmin_private_key }}",
    "maxFeePerGas": "",
    "maxPriorityFeePerGas": "",
    "multiplierGas": "",
    "timelockDelay": 0,
    "timelockSalt": "",
    "rollupManagerAddress": "{{ .zkevm_rollup_manager_address }}",
    "rollupTypeId": 1,
    "_comment": "TODO.. It sounds like isVanilliaClient should be false.. But setting it to false cause other issues. E.g. TypeError: newRollupContract.generateInitializeTransaction is not a function",
    "isVanillaClient": false,
    "sovereignParams": {
        "bridgeManager": "{{ .zkevm_l2_sovereignadmin_address }}",
        "sovereignWETHAddress": "{{ .gas_token_address }}",
        "sovereignWETHAddressIsNotMintable": false,
        "globalExitRootUpdater": "{{ .zkevm_l2_aggoracle_address }}",
        "globalExitRootRemover": "{{ .zkevm_l2_sovereignadmin_address }}",
        "emergencyBridgePauser": "{{ .zkevm_l2_sovereignadmin_address }}"
    },
    "aggchainParams": {
        "aggchainManager": "{{ .zkevm_l2_sovereignadmin_address }}",
        "initParams": {
            "l2BlockTime": "{{ .sp1_l2_block_time }}",
            "rollupConfigHash": "{{.sp1_rollup_config_hash}}",
            "startingOutputRoot": "{{.sp1_starting_output_root}}",
            "startingBlockNumber": {{ with .sp1_starting_block_number }}{{ . }}{{ else }}null{{ end }},
            "startingTimestamp": {{ with .sp1_starting_timestamp}}{{ . }}{{ else }}null{{ end }},
            "submissionInterval": "{{ .sp1_submission_interval }}",
            "optimisticModeManager": "{{ .zkevm_l2_sovereignadmin_address }}",
            "aggregationVkey": "{{ .sp1_aggregation_vkey }}",
            "rangeVkeyCommitment": "{{ .sp1_range_vkey_commitment }}"
        },
        "useDefaultGateway": true,
        "ownedAggchainVKey": "{{ .aggchain_vkey_hash }}",
        "aggchainVKeySelector": "{{ .aggchain_vkey_version }}",
        "initOwnedAggchainVKey": "{{ .aggchain_vkey_hash }}",
        "initAggchainVKeyVersion": "{{ .aggchain_vkey_version }}",
        "vKeyManager": "{{ .zkevm_l2_sovereignadmin_address }}"
    }
}
