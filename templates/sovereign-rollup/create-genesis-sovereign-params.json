{
    "rollupManagerAddress": "{{ .zkevm_rollup_manager_address }}",
    "rollupID": {{ .zkevm_rollup_id }},
    "chainID": {{.zkevm_rollup_chain_id}},
    "gasTokenAddress": "{{ .gas_token_address }}",
    "bridgeManager": "{{ .zkevm_l2_sovereignadmin_address }}",
    "sovereignWETHAddress": "{{ .gas_token_address }}",
    "sovereignWETHAddressIsNotMintable": false,
    "globalExitRootRemover": "{{ .zkevm_l2_sovereignadmin_address }}",
    "emergencyBridgePauser": "{{ .zkevm_l2_sovereignadmin_address }}",
    "emergencyBridgeUnpauser": "{{ .zkevm_l2_sovereignadmin_address }}",
    "proxiedTokensManager" : "{{ .zkevm_l2_sovereignadmin_address }}",
    "setPreMintAccounts": true,
    "preMintAccounts": [
        {
            "balance": "1000000000000000000",
            "address": "0x8576158a89648aA88B6036f47B8b74Fc0C2b5c66"
        }
    ],
    "setTimelockParameters": true,
    "timelockParameters": {
        "adminAddress": "{{ .zkevm_l2_admin_address }}",
        "minDelay": 3600
    },


    {{if .use_agg_oracle_committee}}
    "useAggOracleCommittee": true,
    "aggOracleCommittee": ["{{ .zkevm_l2_aggoracle_address }}"],
    "quorum": "{{ .agg_oracle_committee_quorum }}",
    "aggOracleOwner": "{{ .zkevm_l2_sovereignadmin_address }}",
    "globalExitRootUpdater": "",
    {{else}}
    "useAggOracleCommittee": false,
    "aggOracleCommittee": [],
    "quorum": 0,
    "aggOracleOwner": "{{ .zkevm_l2_sovereignadmin_address }}",
    "globalExitRootUpdater": "{{ .zkevm_l2_aggoracle_address }}",
    {{end}}

    "formatGenesis": "geth"
}
